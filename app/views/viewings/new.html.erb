<%= form_for @viewing do |f| %>

<%= f.label :time %>
<%= f.text_field :time %>

<%= f.label :day %>
<%= f.text_field :day %>

<%= f.label :artwork_id %>
<%= f.collection_select :artwork_id, Artwork.all, :id, :name %>

<%= f.label :user_id %>
<%= f.collection_select :user_id, User.all, :id, :name %>

<%= f.label :comments %>
<%= f.text_field :comments %>

<%= f.submit %>
<%end%>

