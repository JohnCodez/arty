<h1>Enter Artwork</h1>
<%= form_for @artwork do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :size %>
    <%= f.text_field :size %>

    <%= f.label :medium %>
    <%= f.text_field :medium %>

    <%= f.label :gallery_location_id %>
    <%= f.collection_select :gallerylocation_id, Gallerylocation.all, :id, :name %>

    <%= f.label :specialist_id %>
    <%= f.collection_select :specialist_id, Specialist.all, :id, :name %>

    <%= f.submit %>
    <% end %>